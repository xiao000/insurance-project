@model Bx_Core.BxType

@{
    ViewBag.Title = "Detail";
    Layout = null;
}

<div class="wrapper">
	<div class="register-box">
		<div class="register-content">			
                <div class="form-group">
                    @Html.LabelFor(m=>m.BxTypeName,new{style="width:100px;"})@Html.TextBoxFor(m => m.BxTypeName, new { disabled = "false" })
                </div>
                <div class="form-group">
                    @Html.LabelFor(m=>m.CreatDateTime,new{style="width:100px;"})@Html.TextBoxFor(m => m.CreatDateTime, new { disabled = "false" })                   
                </div>
                <div class="form-group">
                    @Html.LabelFor(m=>m.BxRemark,new{style="width:100px;"})@Html.TextAreaFor(m => m.BxRemark, new { disabled = "false" })
                </div>
		</div>
	</div>
</div>