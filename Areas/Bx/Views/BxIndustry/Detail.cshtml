@model Bx_Core.BxIndustry

@{
    ViewBag.Title = "Details";
    Layout = null;
}

<div class="wrapper">
	<div class="register-box">
		<div class="register-content">			
                <div class="form-group">
                    @Html.LabelFor(m=>m.BxIndustryName,new{style="width:100px;"})@Html.TextBoxFor(m => m.BxIndustryName, new { disabled = "false" })
                </div>
                <div class="form-group">
                    @Html.LabelFor(m=>m.CreateTime,new{style="width:100px;"})@Html.TextBoxFor(m => m.CreateTime, new { disabled = "false" })                   
                </div>
                <div class="form-group">
                    @Html.LabelFor(m=>m.BxRemark,new{style="width:100px;"})@Html.TextAreaFor(m => m.BxRemark, new { disabled = "false" })
                </div>
		</div>
	</div>
</div>

